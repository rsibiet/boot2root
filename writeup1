#-------------- Etape 1 --------------
#Download de l'image sur un debian
wget http://dl.42.fr/BornToSecHackMe-v1.1.iso

#Montage de l'iso
mount -o loop ./BornToSecHackMe-v1.1.iso /mnt/iso

#root@ns334883:/mnt/iso/casper# ls -lh
